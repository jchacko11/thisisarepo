{
  "manifest_version": 2,
  "name": "Nithya, Shannon, and Sejal's GC Extension",
  "version": "0.1",
  "content_scripts": [{
      "matches": [
          "<all_urls>"
      ],
      "js": ["content.js"]
  }],
  "background": {
      "scripts": ["background.js"]
  },
  "browser_action": {
      "default_icon": {
          "16": "images/pen16.png",
          "48": "images/pen48.png",
          "128": "images/pen128.png"
      }
  },
  "icons": {
      "16": "images/pen16.png",
      "48": "images/pen48.png",
      "128": "images/pen128.png"
  }
}