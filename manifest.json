{
"manifest_version": 2,
"name": "Nithya, Shannon, and Sejal's GC Extension",
"version": "0.1",
"content_scripts": [{
    "matches": [
          "<all_urls>"
    ],
    "js": ["scripts/content.js"]
  }],
"background": {
    "scripts": ["scripts/background.js"]
    },
"permissions": [
    "activeTab", "storage"
    ],
"icons": {
  "16": "cuteheart2.png",
  "48": "cuteheart2.png",
  "128": "cuteheart2.png"
},
"browser_action": {
    "default_popup": "popup_docs/popup.html"
  }
}
